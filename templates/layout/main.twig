<!DOCTYPE html>
<html class="no-js" lang="ar" dir="rtl">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="/assets/images/icons/favicon.ico" type="image/x-icon">
    <link href="//fonts.googleapis.com/earlyaccess/droidarabickufi.css" rel="stylesheet" type="text/css">
    {% block author %}{% endblock %}
    <meta http-equiv="imagetoolbar" content="no" />
    <meta name="MSSmartTagsPreventParsing" content="true" />
    {% block meta %}{% endblock %}
    <meta name="copyright" content="Diwanee. Copyright (c) 2014" />
    <title>{% block page_title %}{{ profile.site_name }}{% endblock %}</title>

    {% stylesheets
      '../../public/scss/style-ar.scss'
      filter='compass'
      filter='cssmin'
      output='/assets/css/style-ar.css' %}
      <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
    {% endstylesheets %}

    {% stylesheets
      '../../bower_components/raty/lib/jquery.raty.css'
      filter='cssmin'
      output='/assets/css/raty.css' %}
      <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
    {% endstylesheets %}



    {% javascripts
      '../../bower_components/modernizr/modernizr.js'
      filter='jsqueeze'
      output='/assets/js/modernizer.js' %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% if ga is defined %}
    {% include "includes/google-analytics.twig" with { 'accountId': ga.accountId, 'domainName': ga.domainName } %}
    {% endif %}

    {% if debugbarhead is defined %}
    {{ debugbarhead|raw }}
    {% endif %}

    <script>(function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/ar_AR/all.js#xfbml=1";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>
    <!-- Google -->
    <script type="text/javascript" src="https://apis.google.com/js/platform.js"></script>

    <script type="text/javascript">
      Paprika = {
        hostname: '{{ wayyana['url'] }}'
      };
    </script>

    <script type="text/javascript" src="http://ox-d.clickmena.com/w/1.0/jstag"></script>

    <!-- ox js -->
    {% javascripts
      '../../public/js/openx.js'
      '../../bower_components/devicejs/lib/device.js'
      filter='jsqueeze'
      output='/assets/js/ox.js' %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% if ox is defined %}
      {% include "includes/openx.twig" with { 'ox' : ox, 'terms' : terms } %}
    {% endif %}
  </head>
  <body>
    <!-- Facebook -->
    <div id="fb-root"></div>
    {% if debugbarbody is defined %}
    {{ debugbarbody|raw }}
    {% endif %}

    {#{% include 'includes/effective-measure.twig' %}#}

    {% include 'includes/wayyana-topbar.twig'  with { 'wayyanaUrl': wayyana.url, 'originId': wayyana.originId, 'returnUrl': wayyana.returnUrl }%}

    {% include 'blocks/header.twig' %}

    {% block content %} {% endblock %}

    {% include 'blocks/footer.twig' %}

    {#{% include 'includes/ezakus.twig' %}#}

    <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.6.2/html5shiv.js"></script>
    <script src="//s3.amazonaws.com/nwapi/nwmatcher/nwmatcher-1.2.5-min.js"></script>
    <script src="//html5base.googlecode.com/svn-history/r38/trunk/js/selectivizr-1.0.3b.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/respond.js/1.1.0/respond.min.js"></script>
    <script src="js/ie8.min.js" type="text/javascript"></script>
    <![endif]-->

    <!-- js -->
    {% javascripts
      '../../bower_components/jquery/dist/jquery.min.js'
      '../../bower_components/fingerprint/fingerprint.js'
      '../../bower_components/jsSHA/src/sha.js'
      '../../public/js/base64.min.js'
      '../../public/js/hrl.js'
      '../../bower_components/raty/lib/jquery.raty.js'
      '../../bower_components/jquery.lazyload/jquery.lazyload.min.js'
      '../../bower_components/infinite-scroll/jquery.infinitescroll.min.js'
      filter='jsqueeze'
      output='/assets/js/base.js' %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <!-- nojsqueeze -->
    {% javascripts
      '../../bower_components/owlcarousel/owl-carousel/owl.carousel.min.js'
      output='/assets/js/nojsqueeze.js' %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <!-- app -->
    {% javascripts
      '../../public/js/app.js'
      '../../public/js/reviews.js'
      '../../public/js/thumbor.js'
      '../../public/js/infscr.js'
      '../../public/js/social-counter.js'
      '../../public/js/openx-elements.js'
      '../../public/js/widgets/harmony-poll.js'
      filter='jsqueeze'
      output='/assets/js/app.js' %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% include 'includes/piwik.twig' with {'siteId' : 2} %}

    {% include "includes/openx-async.twig" with { 'ox' : ox, 'terms' : terms } %}
    
    <!-- Please call pinit.js only once per page -->
    <script type="text/javascript" src="//assets.pinterest.com/js/pinit.js"></script>
  </body>
</html>
